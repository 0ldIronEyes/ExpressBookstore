{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "book"
    ],
    "properties": {
        "book": {
            "type": "object",
            "default": {},
            "title": "The book Schema",
            "required": [
                "amazon_url",
                "author",
                "language",
                "pages",
                "publisher",
                "title",
                "year"
            ],
            "properties": {
                "amazon_url": {
                    "type": "string",
                    "default": "",
                    "title": "The amazon_url Schema",
                    "examples": [
                        "https://amazon.com/cats"
                    ]
                },
                "author": {
                    "type": "string",
                    "default": "",
                    "title": "The author Schema",
                    "examples": [
                        "cat man"
                    ]
                },
                "language": {
                    "type": "string",
                    "default": "",
                    "title": "The language Schema",
                    "examples": [
                        "English"
                    ]
                },
                "pages": {
                    "type": "integer",
                    "default": 0,
                    "title": "The pages Schema",
                    "examples": [
                        1000
                    ]
                },
                "publisher": {
                    "type": "string",
                    "default": "",
                    "title": "The publisher Schema",
                    "examples": [
                        "publisher"
                    ]
                },
                "title": {
                    "type": "string",
                    "default": "",
                    "title": "The title Schema",
                    "examples": [
                        "Cat Book"
                    ]
                },
                "year": {
                    "type": "integer",
                    "default": 0,
                    "title": "The year Schema",
                    "examples": [
                        2020
                    ]
                }
            },
            "examples": [{
                "amazon_url": "https://amazon.com/cats",
                "author": "cat man",
                "language": "English",
                "pages": 1000,
                "publisher": "publisher",
                "title": "Cat Book",
                "year": 2020
            }]
        }
    },
    "examples": [{
        "book": {
            "amazon_url": "https://amazon.com/cats",
            "author": "cat man",
            "language": "English",
            "pages": 1000,
            "publisher": "publisher",
            "title": "Cat Book",
            "year": 2020
        }
    }]
}